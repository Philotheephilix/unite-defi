# Cross-Chain Swap Environment Variables
# Copy this file to .env and fill in your values

# Your private key (without 0x prefix) - WARNING: Use testnet keys only!
PRIVATE_KEY=f1fa5583f8fb6c0e1d3b82ee7bb81bead1935285cef5a5054594ca727a45ebc1

# RPC URLs for the chains
# Sepolia testnet
SRC_CHAIN_RPC=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
# or use Alchemy: https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Arbitrum Sepolia testnet
DST_CHAIN_RPC=https://sepolia.arbitrum.io/rpc
# or use Alchemy: https://arb-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Optional: Set to true to create forks instead of using live networks
# This is useful for testing without spending real gas fees
SRC_CHAIN_CREATE_FORK=false
DST_CHAIN_CREATE_FORK=false

# Example with public RPC endpoints (may be rate limited):
# SRC_CHAIN_RPC=https://eth.merkle.io
# DST_CHAIN_RPC=https://arbitrum-sepolia.publicnode.com
